<html lang="en">
    <head>
      <title>Login - Software Bug Predictor</title>
      <meta name="title" content="Login - Software Bug Predictor">
      <meta name="description" content="Software Bug Predictor is a service that allows you to predict bugs in software by linking any GitHub repository">
      <meta name="keywords" content="Bugs, GitHub, Software Development">
      <link rel="stylesheet" href="public/css/login.css">
      <link rel="stylesheet" href="public/js/login.js"
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css">
    </head>
    
    <body>
      <div class="section"></div>
      <main>
        <center>
 
          <div class="section"></div>
    
          <h5 class="indigo-text">Please, login into your account</h5>
          <div class="section"></div>
    
          <div class="container">
            <div class="z-depth-1 grey lighten-4 row" style="display: inline-block; padding: 32px 48px 0px 48px; border: 1px solid #EEE;">
    
              <form class="col s12" method="post">
                <div class='row'>
                  <div class='col s12'>
                  </div>
                </div>
    
                <div class='row'>
                  <div class='input-field col s12'>
                    <input class='validate' type='email' name='username' id='username' />
                    <label for='username'>Enter your email</label>
                  </div>
                </div>
    
                <div class='row'>
                  <div class='input-field col s12'>
                    <input class='validate' type='password' name='password' id='password' />
                    <label for='password'>Enter your password</label>
                  </div>
                  <label style='float: right;'>
                    <a class='pink-text' href='#!'><b>Forgot Password?</b></a>
                  </label>
                </div>
    
                <br />
                <center>
                  <div class='row'>
                    <button type='submit' id= 'login-form-submit' name='btn_login' class='col s12 btn btn-large waves-effect indigo'>Login</button>
                  </div>
                </center>
              </form>
            </div>
          </div>
          <a href="signup">Create account</a>
        </center>
        <div class="container">
          <div class="jumbotron text-center text-primary">
              <h1><span class="fa fa-lock"></span> Social Authentication</h1>
              <p>Login or Register with:</p>
              <a href="/auth/google/" class="btn btn-danger"><span class="fa fa-google"></span> SignIn with Google</a>
          </div>
        <div class="section"></div>
        <div class="section"></div>
      </main>
    
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.1/jquery.min.js"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/js/materialize.min.js"></script>
    </body>
    
    
    
